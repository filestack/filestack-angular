{
  "name": "filestack",
  "version": "1.0.2",
  "description": "Angular module for filestack-js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build filestack-angular",
    "test": "ng test filestack-angular --watch=false",
    "test:watch": "ng test filestack-angular",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "ng test filestack-angular --watch=false && ng build filestack-angular && standard-version",
    "publishS3": "npx webpack",
    "publish": "cd dist/filestack-angular/ && npm publish && npm publishS3"
  },
  "dependencies": {
    "@angular/animations": "~9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "~9.1.9",
    "@angular/compiler": "~9.1.9",
    "@angular/core": "~9.1.9",
    "@angular/forms": "~9.1.9",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "~9.1.9",
    "@angular/platform-browser-dynamic": "~9.1.9",
    "filestack-js": "3.x.x",
    "ngx-highlightjs": "^4.1.0-beta",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/build-ng-packagr": "~0.901.7",
    "@angular/cli": "~9.1.7",
    "@angular/compiler-cli": "~9.1.9",
    "@angular/language-service": "~9.1.9",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^9.0.0",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.2",
    "typescript": "~3.8.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-s3-plugin": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filestack/filestack-angular.git"
  }
