{
  "name": "@filestack/angular",
  "version": "1.3.0",
  "description": "Angular module for filestack-js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build filestack-angular --prod",
    "test": "ng test filestack-angular --watch=false",
    "test:watch": "ng test filestack-angular",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "release": "ng test filestack-angular --watch=false && ng build filestack-angular --prod=true && standard-version",
    "publishS3": "npx webpack",
    "publish": "cd dist/filestack-angular/ && npm publish && cd ../../ && npm run publishS3"
  },
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/cdk": "^10.1.3",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "^10.1.3",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "filestack-js": "3.x.x",
    "ngx-highlightjs": "^4.1.1",
    "rxjs": "~6.6.2",
    "tslib": "^2.0.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.15",
    "@angular-devkit/build-ng-packagr": "~0.1000.5",
    "@angular/cli": "~11.2.15",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/language-service": "~11.2.14",
    "@types/jasmine": "~3.5.12",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~6.3.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^11.2.4",
    "protractor": "~7.0.0",
    "standard-version": "^8.0.2",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.3",
    "typescript": "~4.1.6",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-s3-plugin": "^1.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filestack/filestack-angular.git"
  }
}